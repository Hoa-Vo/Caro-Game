<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <script src="socket.io/socket.io.js"></script>
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
    <link rel="stylesheet" type="text/css" href="style.css" />
    <title>Caro game</title>
  </head>
  <body>
    <div id="login-form"><input type="text" id="username" style="font-size: 20px;" /> <button id="btn" style="font-size: 20px;">PLAY</button></div>
    <div id="room-list"></div>
    <div id="status-bar">
      <div id="current-room"></div>
      <button id="exit">EXIT</button>
    </div>
    <div id="wrapper">
      <div id="log-event"></div>
      <div id="box-content"></div>
      <div id="chat-wrap">
        <div id="chat-box"></div>
        <input type="text" id="mess" style="font-size: 20px;" /> <button id="send-mess" style="font-size: 20px;">SEND</button>
      </div>
      <div id="user-login"></div>
    </div>
    <script src="index.js"></script>
  </body>
</html>
