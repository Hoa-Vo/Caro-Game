<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <script src="https://carogame-hoavo.herokuapp.com/socket.io/socket.io.js"></script>
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
    <script src="https://kit.fontawesome.com/a076d05399.js"></script>
    <link rel="stylesheet" type="text/css" href="style.css" />
    <link
      rel="icon"
      href="https://store-images.microsoft.com/image/apps.60140.9007199266250102.3d362e6c-a750-4656-8f24-1e08880daa9f.2e72cda6-f8a9-452c-abb7-0b58a0890525?mode=scale&q=90&h=300&w=300"
    />
    <title>Caro Online</title>
  </head>
  <body>
    <div id="d01">
      <div id="root">
        <div id="login-form"><input type="text" id="username" style="font-size: 20px;" placeholder="Username" /> <button id="btn" style="font-size: 20px;">PLAY</button></div>
        <div id="room-list"></div>
      </div>
      <div id="wrapper">
        <div id="status-bar">
          <div id="current-room"></div>
          <button id="exit">EXIT</button>
        </div>
        <div id="log-event"></div>
        <div id="box-content"></div>
        <div id="chat-wrap">
          <div id="chat-box"></div>
          <div id="nofication"></div>
          <input type="text" id="mess" style="font-size: 20px;" /> <button id="send-mess" style="font-size: 20px;">SEND</button>
        </div>
        <div id="user-login"></div>
      </div>
    </div>
    <script src="index.js"></script>
  </body>
</html>
